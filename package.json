{
  "name": "m365-agent-provisioning",
  "version": "1.0.0",
  "description": "Bulk provisioning tool for Microsoft 365 user accounts with browser-based MSAL authentication",
  "main": "dist/provision.js",
  "type": "module",
  "scripts": {
    "debug:checklist": "node tools/debug/run-checklist.mjs",
    "build": "tsc",
    "build-oid-cache": "npm run build && node dist/build-oid-cache.js",
    "provision": "npm run build && node dist/provision.js",
    "provision:beta": "npm run provision",
    "option-a:enrich": "npm run build && node dist/enrich-profiles.js",
    "option-a:enrich:dry-run": "npm run build && node dist/enrich-profiles.js --dry-run",
    "option-b:setup": "npm run build && node dist/enrich-connector.js --setup",
    "option-b:ingest": "npm run build && node dist/enrich-connector.js",
    "option-b:dry-run": "npm run build && node dist/enrich-connector.js --dry-run",
    "enrich:wait": "npm run build && node dist/wait-for-schema.js",
    "enrich:delete-connector": "node tools/admin/delete-connection.mjs",
    "logout": "npm run build && node dist/provision.js --logout",
    "list-users": "npm run build && node dist/graph-client.js list-users",
    "list-licenses": "npm run build && node dist/graph-client.js list-licenses",
    "update-licenses": "npm run build && node dist/update-licenses.js",
    "reset-password": "npm run build && node dist/reset-password.js",
    "check-mailboxes": "npm run build && node dist/graph-client.js check-mailboxes",
    "test-connection": "npm run build && node dist/graph-client.js test-connection",
    "test-beta": "npm run build && node dist/graph-beta-client.js check-availability",
    "cleanup": "npm run build && node dist/provision.js --cleanup",
    "reset-tenant": "npm run build && node dist/reset-tenant.js",
    "reset-tenant:confirm": "npm run build && node dist/reset-tenant.js --confirm",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "microsoft-365",
    "azure-ad",
    "graph-api",
    "msal",
    "msal-browser",
    "user-provisioning",
    "browser-authentication"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@azure/identity": "^4.0.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "csv-parse": "^5.5.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "open": "^10.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
